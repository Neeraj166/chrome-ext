{
    "manifest_version": 3,
    "name": "ST Utils",
    "version": "1.0",
    "description": "ST Utils",
    "host_permissions": [
        "https://warehouse.voxships.com/*",
        "https://demo.voxmg.com/*",
        "https://demodev.voxmg.com/*",
        "https://demodev.shikhartech.com/*"
    ],
    "permissions": [
        "scripting",
        "tabs",
        "clipboardWrite",
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "icon.png"
        },
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "copy-token": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y"
            },
            "description": "Copy token from current tab"
        }
    }
}